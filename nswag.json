{
  "runtime": "Net90",
  "documentGenerator": {
    "fromDocument": {
      "url": "swagger.json",
      "output": null
    }
  },
  "codeGenerators": {
    "openApiToTypeScriptClient": {
      "className": "{controller}Client",
      "moduleName": "",
      "namespace": "",
      "typeScriptVersion": 4.3,
      "template": "Angular",
      "promiseType": "Promise",
      "httpClass": "HttpClient",
      "injectionTokenType": "InjectionToken",
      "rxJsVersion": 7.0,
      "dateTimeType": "Date",
      "nullValue": "Undefined",
      "generateClientClasses": true,
      "generateClientInterfaces": false,
      "generateOptionalParameters": true,
      "exportTypes": true,
      "wrapDtoExceptions": false,
      "exceptionClass": "ApiException",
      "clientBaseClass": null,
      "wrapResponses": false,
      "wrapResponseMethods": [],
      "generateResponseClasses": false,
      "responseClass": "SwaggerResponse",
      "protectedMethods": [],
      "configurationClass": null,
      "useTransformOptionsMethod": false,
      "useTransformResultMethod": false,
      "generateDtoTypes": true,
      "operationGenerationMode": "MultipleClientsFromFirstTagAndOperationId",
      "markOptionalProperties": true,
      "generateCloneMethod": false,
      "typeStyle": "Interface",
      "classTypes": [],
      "extendedClasses": [],
      "generateDefaultValues": true,
      "generateConstructorInterface": true,
      "generateReadonlyProperties": false,
      "autoImplementConstructorInterface": false,
      "generateBaseUrlProperty": true,
      "baseUrlTokenName": "API_BASE_URL",
      "generateSyncMethods": false,
      "excludeTypeNames": [],
      "replaceTypeNameHints": [],
      "fixControllerCasing": false,
      "fixTypeNameCasing": false,
      "generateControllerNameUsingTags": true,
      "generateOptionalParametersForOpenApi20": false,
      "generateJsonMethodsForOpenApi20": false,
      "generateSingleHttpClientInstance": true,
      "generateHttpClientCreationMethod": false,
      "typeNameGeneratorType": "DefaultTypeNameGenerator",
      "propertyNameGeneratorType": "DefaultPropertyNameGenerator",
      "enumNameGeneratorType": "DefaultEnumNameGenerator",
      "serviceHost": null,
      "serviceSchemes": null,
      "output": "src/app/api/nswag-client.ts"
    }
  }
}
